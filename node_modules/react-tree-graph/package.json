{
  "name": "react-tree-graph",
  "version": "7.0.3",
  "description": "A react library for generating a graphical tree from data using d3",
  "main": "dist/index.js",
  "module": "dist/module/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpb12/react-tree-graph.git"
  },
  "keywords": [
    "d3",
    "graph",
    "react",
    "svg",
    "tree",
    "ui"
  ],
  "author": "James Brierley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jpb12/react-tree-graph/issues"
  },
  "homepage": "https://jpb12.github.io/react-tree-graph",
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-jest": "^27.5.1",
    "coveralls": "^3.1.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.5.1",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rfdc": "^1.3.0",
    "rollup": "^2.67.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-prettier": "^2.2.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "d3-ease": "^2.0.0",
    "d3-hierarchy": "^2.0.0",
    "prop-types": "^15.8.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./__tests__/startup.js"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      ".eslintrc.js",
      "startup.js"
    ]
  },
  "peerDependencies": {
    "react": "^15.3 || ^16 || ^17 || ^18"
  },
  "scripts": {
    "build": "rollup --config",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "eslint": "eslint src __tests__",
    "test": "jest",
    "version": "npm run build"
  },
  "sideEffects": false
}
